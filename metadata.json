{
  "name": "fvoges-pe_backup",
  "version": "1.0.0",
  "author": "fvoges",
  "summary": "Module to manage minimalistic Puppet Enterprise backups",
  "license": "Apache-2.0",
  "source": "https://github.com/fvoges/puppet-pe_backup.git",
  "project_page": "https://github.com/fvoges/puppet-pe_backup",
  "issues_url": "https://github.com/fvoges/puppet-pe_backup/issues",
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.0.0 < 5.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 2.4.0 < 5.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04",
        "16.04"
      ]
    },
    {
      "operatingsystem": "SLES",
      "operatingsystemrelease": [
        "11",
        "12"
      ]
    }
  ]
}
